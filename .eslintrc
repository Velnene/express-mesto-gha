{
  "extends": "airbnb-base",
  "import/no-extraneous-dependencies": [
    "error",
    {
      "devDependencies": true
    }
  ],
  "rules": {
    "linebreak-style": "off",
    "no-console": "warn",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}